#------------------------------------------------------------------------------
VERSION = BWS.01
#------------------------------------------------------------------------------
!ifndef ROOT
ROOT = $(MAKEDIR)\..
!endif
#------------------------------------------------------------------------------
MAKE = $(ROOT)\bin\make.exe -$(MAKEFLAGS) -f$**
DCC = $(ROOT)\bin\dcc32.exe $**
BRCC = $(ROOT)\bin\brcc32.exe $**
#------------------------------------------------------------------------------
PROJECTS = DataCheck.dll DemoDLL1.exe DemoDLL2.exe
#------------------------------------------------------------------------------
default: $(PROJECTS)
#------------------------------------------------------------------------------

DataCheck.dll: DataCheck.dpr
  $(DCC)

DemoDLL1.exe: DemoDLL1.dpr
  $(DCC)

DemoDLL2.exe: DemoDLL2.dpr
  $(DCC)


